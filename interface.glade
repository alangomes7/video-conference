<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.20"/>
  <object class="GtkBuilder" id="builder">
    <interface>
      <!-- interface-requires gtk+ 3.20 -->
      <object class="GtkTextView" id="textview_messages">
        <property name="can_focus">False</property>
        <property name="editable">False</property>
        <property name="cursor_visible">False</property>
        <property name="wrap_mode">GTK_WRAP_WORD</property>
      </object>
      <object class="GtkButton" id="button_run_stop">
        <property name="label" translatable="yes">Run</property>
        <property name="use_action_appearance">False</property>
        <property name="visible">True</property>
        <property name="can_focus">True</property>
        <property name="receives_default">True</property>
        <signal name="clicked" handler="on_button_run_stop_clicked" swapped="no"/>
      </object>
    </interface>
  </object>
</interface>